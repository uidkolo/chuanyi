.logo{
  width: 190rpx;
  height: 76rpx;
  margin: 200rpx auto 0;
}
.title{
  font-size: 28rpx;
  color: #8d8d8d;
  margin: 50rpx 0 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.dot{
  display: block;
  width: 10rpx;
  height: 10rpx;
  background: #8d8d8d;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.login{
  display: flex;
  align-items: center;
  justify-content: center;
  background: #0fb70a;
  color: #fff;
  margin: 0 80rpx;
  font-size: 30rpx;
  height: 90rpx;
  border-radius: 90rpx;
}
.login image{
  width: 59rpx;
  height: 48rpx;
  margin-right: 30rpx;
  position: relative;
  top: 5rpx;
}
.agree{
  border-top: 1rpx solid #e5e5e5;
  padding: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #8d8d8d;
  font-size: 24rpx;
  position: fixed;
  bottom: 0;
  width: 100%;
}
.agree icon{
  margin-right: 20rpx;
}