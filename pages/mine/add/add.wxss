page{
  background: #f5f5f5;
  padding-bottom: 40rpx;
}
.list{
  border-top: 1rpx solid #dcdcdc;
}
.list .item{
  background: #fff;
}
.list .item+.item{
  margin-top: 20rpx;
}
.list .item .top{
  padding: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list .item .top .name{
  font-size: 36rpx;
}
.list .item .top .phone{
  font-size: 32rpx;
}
.list .item .add{
  font-size: 24rpx;
  color: #4d4d4d;
  padding: 0 30rpx 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.list .item .bottom{
  padding: 0 30rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #4d4d4d;
}
.list .item .bottom .radio{
  display: flex;
  align-items: center;
}
.list .item .bottom .radio .icon{
  height: 36rpx;
  width: 36rpx;
  border-radius: 36rpx;
  border: 1rpx solid #999;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 10rpx;
}
.list .item .bottom .radio .icon.active{
  background: #F7C900;
  border: 1rpx solid #F7C900;
}
.list .item .bottom .radio .icon image{
  height: 30rpx;
  width: 30rpx;
}
.list .item .bottom .btns text{
  margin-left: 40rpx;
}

.btn{
  background: #F7C900;
  border-radius: 10rpx;
  height: 92rpx;
  margin: 40rpx 30rpx 0;
  text-align: center;
  line-height: 92rpx;
  color: #1a1a1a;
  font-size: 30rpx;
}